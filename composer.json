{
    "name": "logik/magento2-packages",
    "description": "Logik Magento 2 Packages",
    "type": "magento2-module",
    "license": "Proprietary",
    "repositories": [
        {
            "type": "path",
            "url": ".",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "magento/framework": "*",
        "magento/module-commerce-backend-uix": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "magento/magento2-functional-testing-framework": "^3.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Logik\\Logik\\": ""
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "magento-deploystrategy": "copy"
    }
} 