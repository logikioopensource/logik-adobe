{
    "name": "logik/magento2-packages",
    "description": "Logik Magento 2 Packages",
    "type": "project",
    "license": "Proprietary",
    "require": {
        "logik/module-logik-settings": "dev-master",
        "logik/module-logik-add-to-cart": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "logik/module-logik-settings",
                "version": "dev-master",
                "type": "magento2-module",
                "source": {
                    "type": "git",
                    "url": "https://github.com/logikioopensource/logik-adobe.git",
                    "reference": "master"
                },
                "autoload": {
                    "files": [
                        "LogikSettings/registration.php"
                    ],
                    "psr-4": {
                        "Logik\\LogikSettings\\": "LogikSettings/"
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "logik/module-logik-add-to-cart",
                "version": "dev-master",
                "type": "magento2-module",
                "source": {
                    "type": "git",
                    "url": "https://github.com/logikioopensource/logik-adobe.git",
                    "reference": "master"
                },
                "autoload": {
                    "files": [
                        "AddToCart/registration.php"
                    ],
                    "psr-4": {
                        "Logik\\AddToCart\\": "AddToCart/"
                    }
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
} 