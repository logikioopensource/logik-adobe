{
    "name": "logik/magento2-packages",
    "description": "Logik Magento 2 Packages",
    "type": "magento2-module",
    "license": "Proprietary",
    "require": {
        "php": ">=8.2.0",
        "magento/framework": "*",
        "magento/module-admin-gws": "100.4.*",
        "magento/module-adobe-stock-admin-ui": "1.3.*"
    },
    "autoload": {
        "files": [
            "LogikSettings/registration.php",
            "AddToCart/registration.php"
        ],
        "psr-4": {
            "Logik\\LogikSettings\\": "LogikSettings/",
            "Logik\\AddToCart\\": "AddToCart/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 