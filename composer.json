{
    "name": "logik/magento2-packages",
    "description": "Logik Magento 2 Packages",
    "type": "composer",
    "license": "Proprietary",
    "repositories": [
        {
            "type": "path",
            "url": "./LogikSettings",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./AddToCart",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "logik/module-logik-settings": "*",
        "logik/module-logik-add-to-cart": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 